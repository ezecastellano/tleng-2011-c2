/* Grep line.  */


%{
#include "automata.hpp"
#include "define.hpp"
%}

%token ALFANUM


%% /* Grammar rules and actions follow.  */

input: expression;

expression: 
    | term binary expression
;

term: term unary
    | operand
;

binary: 
    | '|'
;

operand: parenthesis
    | character
;

character: ALFANUM
    | '.'
;

parenthesis: '('expression')'
;

unary: '*'
    | '+'
    | '?'
;


%%
     

